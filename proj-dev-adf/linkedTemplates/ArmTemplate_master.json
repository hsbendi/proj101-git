{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "proj-dev-adf"
		},
		"LS_AmazonS3_secretAccessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'secretAccessKey' of 'LS_AmazonS3'"
		},
		"md_ls_AzureBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'md_ls_AzureBlobStorage'"
		},
		"md_ls_AzureDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'md_ls_AzureDataLakeStorage'"
		},
		"AzureBlobStorage1_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "dev-sa-access-sec"
		},
		"LS_AmazonS3_properties_typeProperties_accessKeyId": {
			"type": "string",
			"defaultValue": "AKIASD7BWL2MF4C2WHP4"
		},
		"LS_AzureBlobStorage_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "dev-sa-access-sec"
		},
		"LS_AzureKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://proj-dev-kv.vault.azure.net/"
		},
		"md_ls_AzureDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://testfilesfolders.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "proj-dev-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_AmazonS3_secretAccessKey": {
						"value": "[parameters('LS_AmazonS3_secretAccessKey')]"
					},
					"md_ls_AzureBlobStorage_connectionString": {
						"value": "[parameters('md_ls_AzureBlobStorage_connectionString')]"
					},
					"md_ls_AzureDataLakeStorage_accountKey": {
						"value": "[parameters('md_ls_AzureDataLakeStorage_accountKey')]"
					},
					"LS_AmazonS3_properties_typeProperties_accessKeyId": {
						"value": "[parameters('LS_AmazonS3_properties_typeProperties_accessKeyId')]"
					},
					"LS_AzureKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_AzureKeyVault_properties_typeProperties_baseUrl')]"
					},
					"md_ls_AzureDataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('md_ls_AzureDataLakeStorage_properties_typeProperties_url')]"
					},
					"AzureBlobStorage1_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('AzureBlobStorage1_properties_typeProperties_connectionString_secretName')]"
					},
					"LS_AzureBlobStorage_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_AzureBlobStorage_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}